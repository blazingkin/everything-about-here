<% if !@data[:google_places].blank? %>
    <% @data[:google_places].each do |place| %>
        <%= image_tag place['icon'] %>
        <br />
        <%= place['name'] %>
        <br />
        <%= render_open_tag(place) %>
        <br />
        <%= distance_to_place(@data, place) %> away
        <br />
    <% end %>
<% end %>